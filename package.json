{
  "name": "simple-weather",
  "version": "1.0.0",
  "description": "A simple weather app.",
  "main": "index.js",
  "scripts": {
    "webpack-build-dev": "webpack -d --watch",
    "webpack-build": "webpack -p",
    "sass-build-dev": "node-sass --watch ./client/src/styles/style.scss ./public/style.css",
    "sass-build": "node-sass ./client/src/styles/style.scss ./public/style.css",
    "start-local": "node -r dotenv/config server/index.js",
    "start-local-nodemon": "nodemon server/index.js",
    "start": "node server/index.js",
    "heroku-postbuild": "npm run webpack-build && npm run sass-build"
  },
  "author": "Jake Ascher",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "dotenv": "^7.0.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.17.2",
    "express": "^4.15.0",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "engines": {
    "node": "10.x"
  }
}
